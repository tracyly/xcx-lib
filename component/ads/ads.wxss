/* 广告页面 */
.flex-center{
  display: flex;
  justify-content: center;
  align-items: center;
}
.flex-between{
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.text-center{
  text-align: center;
}
.ads-container{
  width:100%;
  padding:20rpx 30rpx;
  box-sizing: border-box;
  min-height: 150rpx;
  border-radius: 6rpx;
  color:#333;
  font-size: 30rpx;
  overflow:hidden;
}
image{
  vertical-align: middle;
  width:100%;
  height:100%;
}
/*ads1*/
.ads1,.ads5,.ads6{
  border:1rpx solid #bebebe;
  border-radius: 8rpx;
  min-height: 250rpx;
}
.title{
  padding: 8rpx 12rpx;
  -webkit-font-smoothing: antialiased;
}
/*end ads1*/
/*ads2*/
.ads2-left,.ads2-right{
  width:50%;
  border:1rpx solid #bebebe;
  border-radius: 8rpx;
}
.ads2-left{
  margin-right: 10rpx;
}
.ads2-right{
  margin-left: 10rpx;
}
/*end ads2*/
/*ads3*/
.ads2-middle{
  border:1rpx solid #bebebe;
  border-radius: 6rpx;
  width:50%;
  margin-left: 10rpx;
  margin-right: 10rpx;
}
/*end ads3*/
/*ads4*/
.ads2-middle2{
  border:1rpx solid #bebebe;
  border-radius: 4rpx;
  width:50%;
  margin-left: 5rpx;
  margin-right: 5rpx;
}
.ads4 .ads2-middle{
  margin-left: 5rpx;
  margin-right: 5rpx;
}
.ads4 .ads2-left{
  margin-right: 5rpx;
}
.ads4 .ads2-right{
  margin-left: 5rpx;
}
/*end ads4*/
/*ads5*/
.ads5-left{
  width:66.6%;
}
.ads5-right{
  width:33.4%;
  text-align: center;
  padding:0 25rpx;
  box-sizing: border-box;
}
.ads5-right-top{
  text-align: left;
  display:-webkit-box;
  -webkit-line-clamp:3;/*强制两行显示*/
  -webkit-box-orient:vertical;
  overflow:hidden;
  text-overflow:ellipsis;
  margin-bottom: 20rpx;
}
.see{
  padding:6rpx 22rpx;
  color:#35b480;
  border:2rpx solid #35b480;
  border-radius: 10rpx;
}
/*end ads5*/
/*ads6*/
.ads6 .title{
  width:65%;
}
/*end ads6*/
/*ads7*/
.ads7{
  position: fixed;
  left: 0;
  top:0;
  right:0;
  bottom: 0;
}
.ads7-bg{
  background-color: #000;
   opacity: 0.5; 
}
.ads7-box{
  position: absolute;
  z-index: 1000;
  width:75%;
}
/* .ads7 .title-box{
  background-color: #fff;
} */
.ads7 .title{
  width:60%;
}
.close{
  float:right;
  background-color: #fff;
  width:60rpx;
  height:60rpx;
  line-height: 54rpx;
  text-align: center;
  font-size: 60rpx;
  border-radius: 50%;
  margin-bottom: 50rpx;
}
/*  
为什么 无法内容生成
.ads7-container::before{
  content:'';
  display: table;
  clear: both;
}  */
.ads7 image{
  border-top-left-radius: 10rpx;
  border-top-right-radius:10rpx; 
}
.close:after{
  content: '';
  display: block;
  width:2rpx;
  height:80rpx; 
  margin-left: 29rpx;
  background-color: #fff;
}
.ads7-container{
  border-radius: 10rpx;
  background-color: #fff;
  clear: both;
}
/*end ads7*/
/*ads8*/
.ads8 swiper{
  height:300rpx;
}
.ads8 .swiper{
